<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:semi="https://irihi.tech/semi"
             xmlns:core="clr-namespace:ToDoList.Core"
             x:Class="ToDoList.Core.ToDoListApp"
             RequestedThemeVariant="Default">
    <Application.Resources>
        <ResourceDictionary>
            <core:ServiceLocator x:Key="ServiceLocator" />
        </ResourceDictionary>
    </Application.Resources>
    <Application.Styles>
        <!--<FluentTheme />-->
        <semi:SemiTheme Locale="zh-CN" />
        <semi:SemiPopupAnimations />
        <StyleInclude Source="avares://AvaloniaUtility/Controls/CustomControls.axaml" />
        <StyleInclude Source="avares://ToDoList.Core.Abstract/Controls/CustomControls.axaml" />
    </Application.Styles>
</Application>